{
  "libs": [
    "browser",
    "jquery",
    "chai",
    "react",
    "ecmascript"
  ],
  "ecmaVersion": 6,
  "plugins": {
    "angular": true,
    "webpack": true,
    "complete_strings": true,
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "node": true,
    "node_require": true,
    "commonjs": true,
    "requirejs": true,
    "modules": true,
    "es_modules": true
  },
  "dontLoad": [
      "node_modules/**"
  ]
}
